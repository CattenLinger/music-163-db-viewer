<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox minHeight="-Infinity" minWidth="-Infinity" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="%mediaScanning.title">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label fx:id="labelScanningDirectory" text="Label" />
      <ProgressBar fx:id="progressScanning" maxWidth="1.7976931348623157E308" progress="0.0">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </ProgressBar>
      <Label fx:id="labelCurrentItem" text="Label" />
      <HBox alignment="CENTER_RIGHT">
         <children>
            <Button fx:id="btnInteractive" mnemonicParsing="false" text="btnInteractive" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
